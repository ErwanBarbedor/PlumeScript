/// Test table ewritepand inline
let t = @table
	- write
	- paint
	- song
	quill: mark
end

macro wing(write, paint, z, quill: word)
	$write - $paint - $z - $quill
end

$wing(...t)

/// Output
write - paint - song - mark
/// End

/// Test table set
let t = @table
	- write
	- paint
	- song
end
$t[1]\n
set t[1] = mark
$t[1]
/// Output
write
mark
/// End

/// Test safe index 1
let t = $table(a:b)
$t.a?
$(t.a?)
/// Output
bb
/// End

/// Test safe index 2
let t = $table(a:b)
$t.b?
$(t.b? or 1)
/// Output
1
/// End

/// Test safe index 3
let t = $table(a:b)
$t["a"]?
$(t["a"]?)
/// Output
bb
/// End

/// Test safe index 4
let t = $table(a:b)
$t["b"]?
$(t["b"]? or 1)
/// Output
1
/// End