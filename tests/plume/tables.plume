/// Test table ewritepand inline
let t = @table
	- write
	- paint
	- song
	quill: mark
end

macro wing(write, paint, z, quill: word)
	$write - $paint - $z - $quill
end

$wing(...t)

/// Output
write - paint - song - mark
/// End

/// Test table set
let t = @table
	- write
	- paint
	- song
end
$t[1]\n
set t[1] = mark
$t[1]
/// Output
write
mark
/// End

/// Test destructuration 1
let t = @table
	wing: mark
end
let wing from t
$wing
/// Output
mark
/// End

/// Test destructuration 2
let t = @table
	wing: mark
	paint: song
end
let wing, paint from t
$wing, $paint
/// Output
mark, song
/// End